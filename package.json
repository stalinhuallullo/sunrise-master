{
  "name": "sunrise",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --verbose --env=jsdom",
    "test-dev": "jest --watch --verbose --env=jsdom",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down --volumes",
    "migrate": "npx prisma migrate deploy",
    "prod": "npx --yes prisma migrate deploy --preview-feature && node server.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds/index.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@aws-sdk/client-s3": "^3.51.0",
    "@aws-sdk/s3-request-presigner": "^3.51.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@prisma/client": "^3.9.1",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^4.18.2",
    "dotenv-cli": "^5.0.0",
    "iron-session": "^6.0.5",
    "jsonwebtoken": "^8.5.1",
    "next": "^12.0.10",
    "next-connect": "^0.12.1",
    "next-translate": "^1.2.0",
    "node-fetch": "^3.2.0",
    "node-mocks-http": "^1.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotjar": "^5.1.0",
    "react-icons": "^4.3.1",
    "react-use-intercom": "^1.5.2",
    "sass": "^1.55.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/formidable": "^2.0.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "prettier": "2.5.1",
    "prisma": "^3.9.1",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,tsx}": "eslint --cache --fix",
    "*.{js,tsx,css,scss,md}": "prettier --write"
  }
}
